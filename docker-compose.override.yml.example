# docker-compose.override.yml.example
#
# Docker Compose automatically loads docker-compose.override.yml if it exists.
# Copy this file to docker-compose.override.yml to customize your local setup.
#
# This file is ignored by git, so it's safe for local customizations.
#
# Example overrides:

version: '3.8'

services:
  # Example: Override n8n environment variables
  # n8n:
  #   environment:
  #     - N8N_BASIC_AUTH_ACTIVE=true
  #     - N8N_BASIC_AUTH_USER=myuser
  #     - N8N_BASIC_AUTH_PASSWORD=mypassword
  #     - N8N_HOST=0.0.0.0

  # Example: Change port mappings
  # open-webui:
  #   ports:
  #     - "3001:8080"  # Use port 3001 instead of 3000

  # Example: Add volumes for development
  # flowise:
  #   volumes:
  #     - ./my-local-flowise-data:/app/data
  
  # Example: Override resource limits
  # ollama-cpu:
  #   deploy:
  #     resources:
  #       limits:
  #         cpus: '4'
  #         memory: 8G

  # Example: Add extra hosts
  # n8n:
  #   extra_hosts:
  #     - "api.local:192.168.1.100"